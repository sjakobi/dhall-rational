let Prelude = ./Prelude

let Rational = ./Type

let commonDenominator = ./commonDenominator

let multiply
    : Rational → Rational → Rational
    =   λ(a : Rational)
      → λ(b : Rational)
      → let cd = commonDenominator a b

        in  { numerator = Prelude.Integer.multiply cd.a cd.b
            , predenominator = cd.predenominator
            }

in  multiply
